{
  "name": "githooks",
  "version": "1.0.0",
  "description": "My githooks",
  "main": ".eslintrc.js",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "eslint": "^7.14.0",
    "pre-commit": "^1.2.2",
    "typescript": "^4.1.2"
  },
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "preCommit": "node preCommit.js",
    "start:build": "tsc -w",
    "build": "tsc",
    "prettier:check": "prettier --check src/*",
    "prettier:write": "prettier --write src/*",
    "no-console": "bash dist/noConsole.sh",
    "no-debug": "bash dist/noDebug.sh",
    "no-revert": "zx dist/noRevert.mjs"
  },
  "pre-commit": [
    "lint",
    "prettier:check"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gness1804/githook-pre-commit-standard.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gness1804/githook-pre-commit-standard/issues"
  },
  "homepage": "https://github.com/gness1804/githook-pre-commit-standard#readme",
  "dependencies": {
    "@types/node": "^14.14.10",
    "prettier": "^2.2.0",
    "ts-node": "^9.0.0"
  }
}
